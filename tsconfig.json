{
	"compilerOptions": {
		"experimentalDecorators": true,
		"module": "es2015",
		"noImplicitAny": false,
		"removeComments": true,
		"preserveConstEnums": true,
		"strictNullChecks": false,
		"sourceMap": false,
		"outDir": "./build",
		"target": "es5",
		"lib": ["es2015", "es2016", "es2017", "dom"]
	},
	"include": ["src/**/*", "src/**/**/*", "src/*"],
	"types": ["react"]
}
